{
  "version": 2,
  "functions": {
    "api/*.js": {
      "runtime": "@vercel/node@3.0.7"
    }
  },
  "routes": [
    {
      "src": "/socket.io/(.*)",
      "dest": "/api/socket"
    },
    {
      "src": "/agent",
      "dest": "/agent.html"
    },
    {
      "src": "/user",
      "dest": "/user.html"
    },
    {
      "src": "/",
      "dest": "/user.html"
    }
  ]
} 